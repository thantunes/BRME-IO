{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#espacamento-footer",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#espacamento-footer",
      "flex-layout.row#footer-3-desktop",
      "flex-layout.row#espacamento-footer",
      "flex-layout.row#footer-aviso-desktop",
      "flex-layout.row#footer-4-desktop",
      "flex-layout.row#espacamento-footer",
      "flex-layout.row#footer-5-desktop",
      "flex-layout.row#developers"
      

    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.row#bloconews",
      "vtex.menu@2.x:menu#Products"
     
    ],
    "props": {
      "blockClass": "menu-footer-row",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },


  "footer-spacer":{
    "props": {
      "blockClass": "footer-espaco"
    }
  },






  "flex-layout.row#bloconews": {
    "props": {
      "blockClass": "bloco-home-news"
    },
    "children": ["newsletter-form"]
  },

  "newsletter-form": {

    "props":{

      "block": [
        "ErrorState",
        "SuccessState",
        "LoadingState"
      ]
    },
  
    "children": [
      "rich-text#title-news",
      "newsletter-input-name",
      "newsletter-input-email",
      "newsletter-hidden-field",
      "newsletter-submit",
      "newsletter-checkbox-confirmation"
    ]
  },

  "rich-text#title-news":{
    "props": {
      "text": "### Receba novidades e promoções:",
      "textPosition": "center",
      "textAlignment": "center",
      "blockClass": "titulo-news"
    }
  },


  


"newsletter-input-name":{
  "props": {
    "placeholderText":"Nome"
  }
},


"newsletter-input-email":{
  "props": {
    "placeholderText":"E-mail"
  }
},


  "newsletter-submit":{
    "props": {
      "submitButtonLabel":"Enviar"
    }
  },






  "social-networks": {
